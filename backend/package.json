{
  "name": "jobs-board-backend",
  "version": "2.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "prebuilt": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "yarn run build",
    "serve": "nodemon -w ./src -w ./tests -x yarn run watch",
    "watch": "ts-node src/index.ts",
    "watch:test": "sleep 7 && ./node_modules/.bin/mocha --reporter mochawesome ./tests/*_tests.js",
    "test": "nodemon -w ./src -w ./tests -x yarn run watch:test",
    "travis:test": "./node_modules/.bin/mocha --reporter mochawesome ./tests/*_tests.js"
  },
  "dependencies": {
    "@nuxtjs/composition-api": "^0.33.0",
    "@vue/composition-api": "^1.6.1",
    "body-parser": "^1.19.0",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "helmet": "^5.0.0",
    "ldapts": "^4.0.0",
    "mongodb": "^3.5.2",
    "mysql": "^2.18.1",
    "nodemailer": "^6.3.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.0.7",
    "typeorm": "^0.3.0",
    "vue": "^2.6.14",
    "winston": "^3.2.1"
  },
  "nodemonConfig": {
    "ext": "js, ts, json"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/bson": "4.2.0",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/helmet": "0.0.48",
    "@types/mongodb": "4.0.7",
    "@types/node": "16.11.47",
    "@types/nodemailer": "6.4.4",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "chai": "4.3.6",
    "eslint": "8.21.0",
    "eslint-plugin-node": "11.1.0",
    "mocha": "10.0.0",
    "mochawesome": "7.1.3",
    "supertest": "6.2.4",
    "tslint": "6.1.3",
    "typescript": "4.7.4"
  }
}
